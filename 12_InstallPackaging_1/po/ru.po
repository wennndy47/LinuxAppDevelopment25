#: hangman.c:82
#, c-format
msgid "Hangman Game version %s\n"
msgstr "Версия игры Виселица %s\n"

#: hangman.c:89
#, c-format
msgid "Usage: hangman [OPTION]\n"
msgstr "Использование: hangman [ОПЦИИ]\n"

#: hangman.c:90 hangman.c:92 hangman.c:98 hangman.c:102 hangman.c:107
#, c-format
msgid "\n"
msgstr ""

#: hangman.c:91
#, c-format
msgid "Word guessing game.\n"
msgstr "Игра в угадывание слов.\n"

#: hangman.c:93
#, c-format
msgid "Options:\n"
msgstr "Опции:\n"

#: hangman.c:94
#, c-format
msgid "  -h, --help           display this help and exit\n"
msgstr "  -h, --help          вывести эту помощь и завершить работу\n"

#: hangman.c:95
#, c-format
msgid "  -v, --version        display version information and exit\n"
msgstr "  -v, --version       вывести версию и завершить работу\n"

#: hangman.c:96
#, c-format
msgid "  -l, --language LANG  set game language (en, ru)\n"
msgstr "  -l, --language LANG     установить язык игры (en, ru)\n"

#: hangman.c:97
#, c-format
msgid ""
"  -d, --data-dit DATA_DIR  set directory containing word lists (en, ru)\n"
msgstr ""
"  -d, --data-dit DATA_DIR  установить папку со списком слов (en, ru)\n"

#: hangman.c:99
#, c-format
msgid "Environment variables:\n"
msgstr "Переменные окружения:\n"

#: hangman.c:100
#, c-format
msgid "  LANG                 determines interface language\n"
msgstr "  LANG                определяет язык интерфейса\n"

#: hangman.c:101
#, c-format
msgid "  DATA_DIR                 determines directory with word lists\n"
msgstr "  DATA_DIR            установка папки со списком слов \n"

#: hangman.c:103
#, c-format
msgid "Examples:\n"
msgstr "Примеры:\n"

#: hangman.c:104
#, c-format
msgid "  hangman              start game with default language\n"
msgstr "  hangman             начинает игру с языком по умолчанию\n"

#: hangman.c:105
#, c-format
msgid "  hangman --language ru start game in Russian\n"
msgstr "  hangman --language ru  начинает игру на русском\n"

#: hangman.c:106
#, c-format
msgid "  LANG=ru_RU.UTF-8 hangman  alternative way to set language\n"
msgstr "  LANG=ru_RU.UTF-8 hangman  альтернативный способ задать язык\n"

#: hangman.c:108
#, c-format
msgid "Report bugs to: <me@me.com>\n"
msgstr "Сообщать о багах: <me@me.com>\n"

#: hangman.c:249
#, c-format
msgid "Failed to determine data directory\n"
msgstr "Ошибка в определении директории со списком слов\n"

#: hangman.c:254 hangman.c:258
#, c-format
msgid "Data directory does not exist: %s\n"
msgstr "Директория со списком слов не существует: %s\n"

#: hangman.c:264
#, c-format
msgid "Failed to construct wordlist path\n"
msgstr "Ошибка в конструировании пути к списку слов.\n"

#: hangman.c:270
#, c-format
msgid "Failed to get a word from wordlist.\n"
msgstr "Ошибка в получении слова из списка слов.\n"

#: hangman.c:393
#, c-format
msgid "Word: %s\n"
msgstr "Слово: %s\n"

#: hangman.c:394
